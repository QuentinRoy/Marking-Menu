{
  "env": {
    "test": {
      "sourceMaps": "inline",
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "browsers": ">1%" }, "modules": false, "loose": true }
        ]
      ],
      "plugins": [
        ["@babel/transform-runtime", { "helpers": false, "regenerator": true }]
      ]
    }
  }
}
